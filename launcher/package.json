{
  "name": "base-wars-launcher",
  "productName": "Base Wars",
  "version": "1.0.0",
  "description": "Launch Base Wars",
  "main": "./src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "make": "npm run make-linux && npm run make-mac && npm run make-win",
    "make-linux": "electron-forge package --platform=linux",
    "make-mac": "",
    "make-win": "electron-forge package --platform=win32",
    "publish": "electron-forge publish"
  },
  "keywords": [],
  "author": "andy725",
  "license": "",
  "config": {
    "forge": {
      "packagerConfig": {
        "executableName": "base-wars-launcher",
        "icon": "./src/render/Icon.icns"
      }
    }
  },
  "dependencies": {
    "boostrap": "^2.0.0",
    "electron-squirrel-startup": "^1.0.0",
    "fetch": "^1.1.0",
    "jquery": "^3.6.0",
    "popper.js": "^1.16.1",
    "adm-zip": "^0.5.5",
    "child_process": "^1.0.2",
    "node-fetch": "^2.6.1",
    "request": "^2.88.2"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.54",
    "@electron-forge/maker-zip": "^6.0.0-beta.54",
    "electron": "11.1.1"
  }
}